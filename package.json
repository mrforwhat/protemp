    "build": "vue-cli-service build",
    "dev-build": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "fastclick": "^1.0.6",
    "lib-flexible": "^0.3.2",
